<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.service.parking.parksepeti">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name="com.service.parking.parksepeti.ParkSepeti"
        android:icon="@drawable/ic_dashboard_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bg_rectangle_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.service.parking.parksepeti.Controller.Activity.BirincilActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.service.parking.parksepeti.Controller.Activity.GooglePinParkYeriKiralama"/>
        <activity android:name="com.service.parking.parksepeti.Controller.Activity.ParkYeriGiris.AdresSecActivity" />

        <activity android:name="com.service.parking.parksepeti.Controller.Activity.KullanıcıKaydet.MobileVerifyActivity" />
        <activity android:name="com.service.parking.parksepeti.Controller.Activity.KullanıcıKaydet.OtpVerifyActivity" />
        <activity android:name="com.service.parking.parksepeti.Controller.Activity.KullanıcıProfiliActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name="com.service.parking.parksepeti.Controller.Activity.ParkYeriGiris.ParkYeriPinleActivity" />
    </application>

</manifest>